<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.alongking.wechat.entity.EcmCategoryStoreEntity" table="ecm_category_store" schema="fenshang">
        <composite-id mapped="true" class="com.alongking.wechat.entity.EcmCategoryStoreEntityPK">
            <key-property name="cateId">
                <column name="cate_id" sql-type="int(10) unsigned"/>
            </key-property>
            <key-property name="storeId">
                <column name="store_id" sql-type="int(10) unsigned"/>
            </key-property>
        </composite-id>
    </class>
</hibernate-mapping>