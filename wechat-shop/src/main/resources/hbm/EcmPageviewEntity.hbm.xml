<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.alongking.wechat.entity.EcmPageviewEntity" table="ecm_pageview" schema="fenshang">
        <id name="recId">
            <column name="rec_id" sql-type="int(10) unsigned"/>
        </id>
        <property name="storeId">
            <column name="store_id" sql-type="int(10) unsigned"/>
        </property>
        <property name="viewDate">
            <column name="view_date" sql-type="date"/>
        </property>
        <property name="viewTimes">
            <column name="view_times" sql-type="int(10) unsigned"/>
        </property>
    </class>
</hibernate-mapping>