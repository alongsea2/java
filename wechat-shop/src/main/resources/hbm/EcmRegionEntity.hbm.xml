<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.alongking.wechat.entity.EcmRegionEntity" table="ecm_region" schema="fenshang">
        <id name="regionId">
            <column name="region_id" sql-type="int(10) unsigned"/>
        </id>
        <property name="regionName">
            <column name="region_name" sql-type="varchar(100)" length="100"/>
        </property>
        <property name="parentId">
            <column name="parent_id" sql-type="int(10) unsigned"/>
        </property>
        <property name="sortOrder">
            <column name="sort_order" sql-type="int(11) unsigned"/>
        </property>
    </class>
</hibernate-mapping>