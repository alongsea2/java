<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.alongking.wechat.entity.EcmRecommendEntity" table="ecm_recommend" schema="fenshang">
        <id name="recomId">
            <column name="recom_id" sql-type="int(10) unsigned"/>
        </id>
        <property name="recomName">
            <column name="recom_name" sql-type="varchar(100)" length="100"/>
        </property>
        <property name="storeId">
            <column name="store_id" sql-type="int(10) unsigned"/>
        </property>
    </class>
</hibernate-mapping>