<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.alongking.wechat.entity.EcmCouponSnEntity" table="ecm_coupon_sn" schema="fenshang">
        <id name="couponSn">
            <column name="coupon_sn" sql-type="varchar(20)" length="20"/>
        </id>
        <property name="couponId">
            <column name="coupon_id" sql-type="int(10) unsigned"/>
        </property>
        <property name="remainTimes">
            <column name="remain_times" sql-type="int(10)"/>
        </property>
    </class>
</hibernate-mapping>