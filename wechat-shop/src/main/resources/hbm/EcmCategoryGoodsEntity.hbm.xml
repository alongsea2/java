<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.alongking.wechat.entity.EcmCategoryGoodsEntity" table="ecm_category_goods" schema="fenshang">
        <composite-id mapped="true" class="com.alongking.wechat.entity.EcmCategoryGoodsEntityPK">
            <key-property name="cateId">
                <column name="cate_id" sql-type="int(10) unsigned"/>
            </key-property>
            <key-property name="goodsId">
                <column name="goods_id" sql-type="int(10) unsigned"/>
            </key-property>
        </composite-id>
    </class>
</hibernate-mapping>