<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.alongking.wechat.entity.EcmUserCouponEntity" table="ecm_user_coupon" schema="fenshang">
        <composite-id mapped="true" class="com.alongking.wechat.entity.EcmUserCouponEntityPK">
            <key-property name="userId">
                <column name="user_id" sql-type="int(10) unsigned"/>
            </key-property>
            <key-property name="couponSn">
                <column name="coupon_sn" sql-type="varchar(20)" length="20"/>
            </key-property>
        </composite-id>
    </class>
</hibernate-mapping>